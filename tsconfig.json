{
  "compilerOptions": {
   "target": "es5",                                 
    "lib": [
      "DOM","DOM.Iterable","ESNext"
    ],                                        
    "esModuleInterop": true,                          
    "module": "esnext",                                                   
    "jsx": "react",  
    "rootDir": "src",
    "baseUrl": "src",
    "allowJs": true,                             
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,                                     
    "forceConsistentCasingInFileNames": true,           
    //"isolatedModules":true,
    "noEmit": true,                                   
  },
  "include": [
    "src"
  ]
}
